{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "BASE_URL": "https://$VERCEL_URL"
    }
}